{
  "name": "backend",
  "version": "1.0.0",
  "description": "WarhammerLibrary_Backend",
  "main": "index.js",
  "scripts": {
    "startBack": "concurrently \"npm run backDev\" \"npm run backLibrary\" \"npm run backMedia\" \"npm run backAuth\"",
    "start": "react-scripts start",
    "backDev": "json-server --watch db.json --port 3001 & json-server --watch notes.json --port 3002",
    "backLibrary": "json-server --watch library.json --port 3003",
    "backMedia": "json-server --watch empty.json -s ./src/assets --port 3004",
    "backAuth": "json-server-auth db.json -r routes.json --port 3005"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "util": "^0.12.5",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "json-server": "^0.17.4",
    "json-server-auth": "^2.1.0"
  }
}
